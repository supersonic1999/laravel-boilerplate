{
    "name": "rappasoft/laravel-boilerplate",
    "type": "project",
    "description": "The Laravel Boilerplate Project.",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "arcanedev/log-viewer": "^9",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.8.1",
        "jamesmills/laravel-timezone": "^1.13",
        "lab404/laravel-impersonate": "^1.7.4",
        "langleyfoxall/laravel-nist-password-rules": "^6.1",
        "laragear/two-factor": "^1.2.2",
        "laravel/cashier": "*",
        "laravel/framework": "^9.52.16",
        "laravel/sanctum": "^2.15.1",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8.2",
        "laravel/ui": "^3.4.6",
        "livewire/livewire": "^2.12.6",
        "rappasoft/laravel-livewire-tables": "^1.25.1",
        "rappasoft/lockout": "^4",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-permission": "^5.11.1",
        "tabuna/breadcrumbs": "^3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9.2",
        "barryvdh/laravel-ide-helper": "^2.13",
        "brianium/paratest": "^6.11",
        "spatie/laravel-ignition": "^1.6.4",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^6.4",
        "laravel/sail": "^1.26.3",
        "friendsofphp/php-cs-fixer": "^3.45",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "test": "@php artisan test --parallel",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
